<% if current_coder %>
  <li class='nav-text pull-right' style='color: white'>Signed in as <%= current_coder.coder_name %></li>
  <li><%= link_to 'Dashboard', dashboard_path(current_coder.id) %></li>
  <li><%= link_to 'Logout', logout_path, method: :delete %></li>
<% else %>
  <% if session[:coders_email] %>
    <li><%= link_to 'Coder Dashboard', view_coder_path, method: 'POST' %></li>
  <% end %>
  <li><%= link_to 'Signup', new_coder_path %></li>
  <li><%= link_to 'Login', login_path %></li>
<% end %>
