<% if current_coder %>
  <li class='nav-text'>Signed in as <%= current_coder.coder_name %></li>
  <li><%= link_to 'Dashboard', dashboard_path(current_coder.id) %></li>
  <li><%= link_to 'Logout', logout_path, method: :delete %></li>
<% else %>
  <li><%= link_to 'Signup', new_coder_path %></li>
  <li><%= link_to 'Login', login_path %></li>
<% end %>
