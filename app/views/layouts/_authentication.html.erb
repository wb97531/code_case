<% if current_coder %>
  <li class="pad-link"><%= link_to "#{current_coder.coder_name}", coder_path(current_coder.id) %></li>
  <li class="pad-link"><%= link_to 'Display', dashboard_path(current_coder.id) %></li>
  <li class="pad-link"><%= link_to 'Logout', logout_path, method: :delete %></li>
<% else %>
  <% if session[:coders_email] %>
    <li class="pad-link"><%= link_to 'Display', view_coder_path, method: 'POST' %></li>
    <li class="pad-link"><%= link_to 'Done', logout_path, method: :delete %></li>
  <% end %>
<% end %>
