<div class="page-header">
  <%= link_to snippets_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_snippet_path(@snippet), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show snippet</h1>
</div>

<dl class="dl-horizontal">
  <dt>Objective:</dt>
  <dd><%= @snippet.objective %></dd>

  <dt>Image name:</dt>
  <dd><%= @snippet.image_name %></dd>

  <dt>Github file link:</dt>
  <dd><%= @snippet.github_file_link %></dd>

  <dt>Project:</dt>
  <dd><%= @snippet.project_id %></dd>

</dl>
